<mapper namespace="com.zerock.mapper.BoardMapper">

<select id="getList" resultType="com.zerock.domain.BoardVO">
<![CDATA[select * from board]]>
</select>

 <insert id="insert">
	INSERT INTO BOARD("bno", "title", "detail") 
	VALUES (tmp_seq.nextval, #{title} , #{detail})
</insert>

<delete id="delete">
	delete from board where bno = #{bno}
</delete>
 
</mapper>